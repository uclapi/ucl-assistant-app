{
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx --cache --ignore-path .eslintignore",
    "lint:force": "eslint . --ext .js --ext .jsx --ignore-path .eslintignore",
    "lint:staged": "lint-staged",
    "format": "prettier --write --trailing-comma all **/*.{js,jsx}",
    "test": "jest --ci && yarn run lint:force",
    "storybook": "storybook start -p 7007",
    "prestorybook": "rnstl",
    "start": "expo start",
    "publish:prod": "expo publish --release-channel=production",
    "publish:dev": "expo publish --release-channel=development"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.2",
    "axios": "^0.19.0",
    "base-64": "^0.1.0",
    "d3-scale": "^3.0.0",
    "d3-shape": "^1.3.5",
    "expo": "^33.0.6",
    "expo-analytics-amplitude": "^5.0.1",
    "expo-asset": "^5.0.1",
    "expo-blur": "^5.0.1",
    "expo-constants": "^5.0.1",
    "expo-font": "^5.0.1",
    "expo-intent-launcher": "^5.0.1",
    "expo-linear-gradient": "^5.0.1",
    "expo-localization": "^5.0.1",
    "expo-mail-composer": "^5.0.1",
    "expo-permissions": "^5.0.1",
    "expo-secure-store": "^6.0.0",
    "expo-web-browser": "^5.0.3",
    "lottie-react-native": "^2.6.1",
    "memoize-one": "^5.0.5",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-moment-proptypes": "^1.6.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-action-button": "^2.8.5",
    "react-native-maps": "^0.24.2",
    "react-native-modal-datetime-picker": "^7.5.0",
    "react-native-svg": "^9.4.0",
    "react-native-svg-charts": "^5.2.0",
    "react-navigation": "github:uclapi/react-navigation",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-persist-expo-securestore": "github:uclapi/redux-persist-expo-securestore",
    "redux-thunk": "^2.3.0",
    "sentry-expo": "~1.13.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/react-native": "^5.1.9",
    "babel-eslint": "^10.0.2",
    "babel-preset-expo": "^5.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-cli": "^3.0.4",
    "husky": "^3.0.1",
    "jest-expo": "^33.0.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "react-dom": "16.0.1",
    "react-native-storybook-loader": "^1.6.0",
    "react-test-renderer": "^16.8.6",
    "redux-logger": "^3.0.6"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --trailing-comma all",
      "git add"
    ]
  },
  "jest": {
    "testURL": "http://localhost/",
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|sentry-expo|react-native-action-button|redux-persist.*))"
    ],
    "testEnvironment": "node"
  },
  "license": "MIT",
  "name": "ucl-assistant-app",
  "version": "1.1.2",
  "description": "An app to manage your UCL life",
  "author": "Matt Bell",
  "config": {
    "react-native-storybook-loader": {
      "pattern": "**/*-stories.js"
    }
  },
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint:staged"
    }
  }
}
