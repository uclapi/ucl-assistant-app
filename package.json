    "@react-navigation/material-top-tabs": "^5.2.18",
    "@react-navigation/native": "^5.7.5",
    "@react-navigation/stack": "^5.9.2",
    "axios": "^0.20.0",
    "base-64": "^0.1.0",
    "expo": "^39.0.0",
    "expo-analytics-amplitude": "~8.3.1",
    "expo-asset": "~8.2.0",
