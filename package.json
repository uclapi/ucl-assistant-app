    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "axios": "^0.21.0",
    "base-64": "^1.0.0",
    "expo": "^39.0.5",
    "expo-analytics-amplitude": "~8.3.1",
    "expo-asset": "~8.2.0",
