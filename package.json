{
  "name": "ucl-assistant-app",
  "version": "1.1.2",
  "private": true,
  "description": "An app to manage your UCL life",
  "license": "MIT",
  "author": "UCL API",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build": "node scripts/build.js",
    "format": "eslint . --fix --ext .js --ext .jsx --cache --ignore-path .eslintignore",
    "lint:staged": "lint-staged",
    "publish": "node scripts/publish.js",
    "start": "expo start",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007",
    "test": "jest --ci && codecov",
    "upload": "node scripts/upload.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "pattern": "**/*-stories.js"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|jest-expo/.*|@unimodules/.*|sentry-expo|native-base|react-native-action-button|redux-persist.*)"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.5",
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "base-64": "^0.1.0",
    "d3-scale": "^3.1.0",
    "d3-shape": "^1.3.5",
    "expo": "^34.0.4",
    "expo-analytics-amplitude": "^6.0.0",
    "expo-asset": "^6.0.0",
    "expo-blur": "^6.0.0",
    "expo-constants": "^6.0.0",
    "expo-font": "^6.0.1",
    "expo-intent-launcher": "^6.0.0",
    "expo-linear-gradient": "^6.0.0",
    "expo-localization": "^6.0.0",
    "expo-mail-composer": "^6.0.0",
    "expo-permissions": "^6.0.0",
    "expo-secure-store": "^6.0.0",
    "expo-web-browser": "^6.0.0",
    "lottie-react-native": "~2.6.1",
    "memoize-one": "^5.1.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-moment-proptypes": "^1.7.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.1.tar.gz",
    "react-native-action-button": "^2.8.5",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-maps": "~0.24.0",
    "react-native-modal-datetime-picker": "^7.5.0",
    "react-native-reanimated": "^1.1.0",
    "react-native-svg": "~9.5.1",
    "react-native-svg-charts": "^5.3.0",
    "react-navigation": "github:uclapi/react-navigation",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-persist": "^5.10.0",
    "redux-persist-expo-securestore": "github:uclapi/redux-persist-expo-securestore",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sentry-expo": "~1.13.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/react-native": "^5.1.11",
    "babel-eslint": "^10.0.3",
    "babel-preset-expo": "^5.2.0",
    "codecov": "^3.5.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-no-secrets": "^0.3.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-native": "^3.7.0",
    "eslint-plugin-security": "^1.4.0",
    "expo-cli": "^3.0.10",
    "husky": "^3.0.5",
    "inquirer": "^7.0.0",
    "jest-expo": "^34.0.1",
    "lint-staged": "^9.2.5",
    "react-dom": "16.8.6",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "^16.9.0",
    "redux-logger": "^3.0.6"
  }
}
